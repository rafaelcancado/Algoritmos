#include <stdio.h>

int main()
{
    int km, dias, prcd;
    float prckm, prct;
    
    
    printf("Quantos KM você percorreu: ");
    scanf("%d", &km);
    printf("Por quantos dias você utilizou o carro: ");
    scanf("%d", &dias);
    
    prcd = 90;
    prckm = 0.20;
    prct = prckm * km + prcd * dias;
    
    printf("O preço a pagar é %f", prct);
    
    
    
    
    
    

    return 0;
}
